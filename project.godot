; Engine configuration file.
; It's best edited using the editor UI and not directly,
; but you can still tweak values manually.
; This file will be regenerated by Godot when you import a project.

config_version=5

[application]

config/name="Steampunk Train RPG"
config/description="A 16-bit side-scrolling action RPG set aboard a transcontinental steampunk train"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[rendering]

textures/canvas_textures/default_texture_filter=0
renderer/rendering_method="forward_plus"
